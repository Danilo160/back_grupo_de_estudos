

{
    "version": 2,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "cache-control",
                    "value": "s-maxage=600"
                },
                {
                    "key": "access-control-allow-origin",
                    "value": "*"
                }
            ]
        }
    ],
      "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {

      "src": "/(.*)",

      "dest": "index.js"
    }
  ]
}



